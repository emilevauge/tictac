{
  "id": "tictac",
  "cpus": 0.1,
  "mem": 64.0,
  "instances": 3,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "emilevauge/tictac",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/health",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
